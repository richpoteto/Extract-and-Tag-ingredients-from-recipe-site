<script>
  (function(win){
    if (typeof win.dry === 'undefined') win.dry = {};
    var debug = false;
    var topics = {};
    var hOP = topics.hasOwnProperty;
    function createTopic(topic) {
      topics[topic] = {
        data: null,
        subscribers: []
      };
    }
    win.dry.events = {
      subscribe: function(topic, listener) {
        // if topic doesn't exist, create it
        if(!hOP.call(topics, topic)) createTopic(topic);
        var index = topics[topic].subscribers.push(listener) - 1;
        if (debug) console.log('subscribe', topic, topics[topic].data, listener);
        var data = topics[topic].data;
        // if data already populated, call immediately
        if (data) listener(data);
        // return an unsub handle
        return {
          remove: function() {
            delete topics[topic].subscribers[index];
          }
        };
      },
      publish: function(topic, data) {
        // if topic doesn't exist, create it
        if(!hOP.call(topics, topic)) createTopic(topic);
        // assign the data to the topic
        if (data) topics[topic].data = data;
        if (debug) console.log('publish', topic, topics[topic].data);
        // iterate over subscribers and send the data
        topics[topic].subscribers.forEach(function(item) {
          if (typeof item === 'function') {
            item(data != undefined ? data : {});
          }
        });
      }
    };
  })(window);
</script>

<script>
if ('serviceWorker' in navigator) {
  navigator.serviceWorker.register("/service_worker.js").catch(function() {});
}
</script>


